{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "client",
      "image": "mkkos123/multi-client",
      "hostname": "client",
      "essential": "false"
    },
    {
      "name": "server",
      "image": "mkkos123/multi-server",
      "hostname": "api",
      "essential": "false"
    },
    {
      "name": "sworker",
      "image": "mkkos123/multi-worker",
      "hostname": "worker",
      "essential": "false"
    },
    {
      "name": "nginx",
      "image": "mkkos123/multi-nginx",
      "hostname": "nginx",
      "essential": "true",
      "portMappings":[{
        "hostPort": 80,
        "containerPort": 80
      }],
      "links": ["client", "server"]
    }
  ]
}
